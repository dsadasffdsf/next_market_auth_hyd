{
  "compilerOptions": {
  "target": "es6",
  "module": "commonjs",
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true,
  "jsx": "preserve",
  "lib": ["dom", "dom.iterable", "esnext"],
  "allowJs": true,
  "strict": false,
  "noEmit": true,
  "incremental": true,
  "moduleResolution": "node",
  "isolatedModules": true,
  "baseUrl": ".",
  "paths": {
    "@redux/*": ["src/redux/*"],
    "@components/*": ["src/components/*"],
    "@interfaces/*": ["src/interfaces/*"],
    "@/*": ["./src/shadcn/*"],
    "@data/*": ["./src/data/*"],
    "@configs/*": ["src/configs/*"],
    "@controllers/*": ["src/controllers/*"],
    "@http/*": ["src/http/*"]
  },
  "plugins": [
    {
      "name": "next"
    }
  ]
},
"include": [
  "src/**/*",
  "next-env.d.ts",
  ".next/types/**/*.ts",
  "src/auth.config.ts",
  "src/jest.setup.js",
  "babel.config.js",
  "jest.config.js",
  "middleware.ts"
],
"exclude": [
  "node_modules",
  ".next",
  "dist"
]
}
